absolute-full()
	position absolute
	top 0
	right 0
	bottom 0
	left 0

/* CSS declarations go here */
*
	user-select none
	-webkit-user-select none
	padding 0
	margin 0
	list-style none
	border none
	outline none
	-webkit-appearance none
html, body
	margin 0
	padding 0
	background black
.icon
	absolute-full()
	width 100%
	height 100%
#pFirst
	fill #fff
#pFirstFill
	fill #fff
#pLast
	fill #fff
#pLastFill
	fill #fff
#videoWrapper
	absolute-full()
#video
	absolute-full()

/* Hide Video Elements */
video, .OT_publisher, .OT_root
	display none!important

/* Upscale canvas for performance sakes */
canvas
	position absolute!important
	top 0!important
	right 0!important
	bottom 0!important
	left 0!important
	width 100%!important
	height 100%!important

/*Temporary Styles for Testing p tag centering*/
p
	line-height 40px
input
	border none
	outline none
	-webkit-appearance none

/* Fonts */
